<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
	<bean id="userImpl" class="dong.shopping.impl.UsersImpl">
		<property name="hibernateTemplate" ref="hibernateTemplate"></property>
	</bean>
	
	<bean id="categoryImpl" class="dong.shopping.impl.CategoryImpl">
		<property name="hibernateTemplate" ref="hibernateTemplate"></property>
	</bean>
	
	<bean id="goodsImpl" class="dong.shopping.impl.GoodImpl"> 
	<property name="hibernateTemplate" ref="hibernateTemplate"></property>
	</bean>
	
	<bean id="forderImpl" class="dong.shopping.impl.ForderImpl">
	<property name="hibernateTemplate" ref="hibernateTemplate"></property>
	</bean>
	
	<bean id="accountImpl" class="dong.shopping.impl.AccoutImpl">
	<property name="hibernateTemplate" ref="hibernateTemplate"></property>
	</bean>
</beans>